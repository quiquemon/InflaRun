    function load(page){
		$("#response-container").fadeIn('slow');
                $.get("<?php echo $this -> basePath(); ?>/sendinblue/displayListUsers/<?php echo $this ->id; ?>", {
				pagina: page
			},  function(response) {
				if (response.estatus !== 0) {
                                       
                                        var TotalContactos = response.data.total_list_records;
                                        var adjacents  = 2; //brecha entre páginas después de varios adyacen
                                        var per_page = 50;
                                        
                                        var tpages = Math.ceil(TotalContactos/per_page);
                                        var output;
                                        $("#pagina ").text(page);
                                        //$("#inflaruners").prepend(output);
                                        $(".progress").remove();
                                        $.each(response.data.data, function( key, value ) {
                                            output += "<tr class='contents-row '>";
                                            output += "<td class='row-head profile-select' style='width: 40px;'>";
                                                output += "<div class='checkbox checkbox-primary'>";
                                                output += "<input type='checkbox' id='sib-" + value['id'] + "' data-index='0' name='userIDs[]' value='3901'>";
                                                output += "<label for='sib-" + value['id'] + "'>";
                                                output += "</label>";
                                                output += "</div>";
                                                output += "</td>";
                                            output += "<td class=' row-head' title='Email' style='width:314px;'>";
                                                output += "<div class=' properity'>";
                                                output += "<a href='javascript:void(0);' index='0' class='openpopup'>"+ value['email'];
                                                output += "</a>";
                                                output += "</div>";
                                                output += "</td>";
                                           /*
                                                output += "<td class='' title='NOMBRE' style='width:160px;'>";
                                                output += "<div class=' properity'>"+ "Mónica Gabriela";
                                                output += "</div>";
                                                output += "</td>";
                                                
                                             output += "<td class='' title='SMS' style='width:69px;'>";
                                                output += "<div class=' properity'>";
                                                output += "</div>";
                                                output += "</td>";
                                                output += "<td class='' title='SURNAME' style='width:125px;'>";
                                                output += "<div class=' properity'>"+"Prendes";
                                                output += "</div>";
                                                output += "</td>";
                                            output += "<td class='' title='SURNAME2' style='width:104px;'>";
                                                output += "<div class=' properity'>"+0;
                                                output += "</div>";
                                                output += "</td>";*/
                                            output += "<td class='' title='Última modificación' style='width:181px;'>";
                                                output += "<div class=' properity'>"+ value['last_modified'];
                                                output += "</div>";
                                                output += "</td>";
